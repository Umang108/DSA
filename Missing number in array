class Solution{
  public:
    int MissingNumber(vector<int>& array, int n) {
        // Your code goes here
        // vector<int>ans;
        long long int total= n*(n+1)/2;
        long long int sum=0;
        for(int i=0;i<n-1;i++){
            sum+=array[i];
        }
        return total-sum;
    }
};
