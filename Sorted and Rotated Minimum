class Solution {
  public:
    int findMin(vector<int>& arr) {
        // complete the function here
        // sort(arr.begin(),arr.end());
        // return arr[0];
        
        int min = arr[0];
        int n= arr.size();
        for(int i=1; i<n;i++){
            if(arr[i]<min){
                min  = arr[i];
            }
        }
        return min;
    }
};
