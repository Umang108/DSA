class Solution {
  public:
    bool isPalinSent(string &s) {
        // code here
        string filter;
        
        for(auto str : s){
            if(isalnum(str)){
                filter += tolower(str);
            }
        }
        
        int left =0;
        int right = filter.size() - 1;
        while(left < right){
            if(filter[left] != filter[right]){
                return false;
            }
            left ++;
            right --;
            
        }
        return true;
    }
};
