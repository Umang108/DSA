class Solution {
  public:

    // Note that the size of the array is n-1
    int missingNumber(int n, vector<int>& arr) {
        // Your code goes here
        int totalsum=n*(n+1)/2;
        int arrsum=0;
        for(int num : arr){
            arrsum+= num;
        }
        return totalsum - arrsum;
    }
};
