class Solution {
public:
    bool hasSameDigits(string s) {
        string a = "";
        int i = 0;
        while(s.size() > 2){
            if(i == s.size() - 1){
                s = a;
                i = 0;
                a = "";
                continue;
            }
            int curr = s[i] - '0';
            int next = s[i+1] - '0';
            
            int newDigits = (curr + next)%10;
            a += (newDigits + '0');
            i++;
        }
        return s[0] == s[1];
    }
};
